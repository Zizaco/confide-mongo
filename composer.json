{
    "name": "zizaco/confide-mongo",
    "description": "Confide Mongo is a authentication solution for Laravel that uses mongolid-laravel",
    "keywords": ["laravel","illuminate","auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-mongodb": "^1.2.0",
        "illuminate/contracts": "^5.0",
        "illuminate/support": "^5.0",
        "illuminate/mail": "^5.0",
        "leroy-merlin-br/mongolid-laravel": "2.0.0-beta4",
        "zizaco/confide": "5.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.4",
        "mockery/mockery": "^0.9",
        "illuminate/container": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Zizaco\\ConfideMongo\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}

{
    "name": "zizaco/confide-mongo",
    "description": "Confide Mongo is a authentication solution for Laravel that uses mongolid-laravel",
    "keywords": ["laravel","illuminate","auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/leroy-merlin-br/confide"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-mongodb": "^1.2.0",
        "illuminate/contracts": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/support": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "illuminate/mail": "^5.8 || ^6.0 || ^7.0 || ^8.0",
        "leroy-merlin-br/mongolid-laravel": "^2.0",
        "zizaco/confide": "6.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mockery/mockery": "^1.0",
        "illuminate/container": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {
            "Zizaco\\ConfideMongo\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}

{
    "name": "zizaco/confide-mongo",
    "description": "Confide Mongo is a authentication solution for Laravel 4 that uses mongolid-laravel",
    "keywords": ["laravel","illuminate","auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zizaco Zizuini",
            "email": "zizaco@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.*",
        "zizaco/mongolid-laravel": "*",
        "zizaco/confide": "*"
    },
    "require-dev": {
        "mockery/mockery": "0.7.2",
        "illuminate/database": "4.*",
        "illuminate/auth": "4.*"
    },
    "autoload": {
        "classmap": [
            "src/commands"
        ],
        "psr-0": {
            "Zizaco\\ConfideMongo": "src/"
        }
    },
    "minimum-stability": "dev"
}
